{
  "name": "react-courses-adm",
  "version": "1.0.0",
  "private": false,
  "description": "React and Redux example",
  "main": "index.js",
  "scripts": {
    "pull": "git pull && yarn && yarn outdated",
    "prestart": "yarn lint:scss",
    "start": "pm2 start process.json",
    "poststart": "run-p watch:*",
    "stop": "pm2 stop process.json",
    "poststop": "pm2 flush; pm2 delete all; exit 0",
    "plist": "pm2 l",
    "lint:js": "esw src",
    "lint:scss": "stylelint \"src/client/**/*.scss\" --syntax scss",
    "lint": "run-s lint:*",
    "watch:lint": "yarn lint:js --watch",
    "test": "jest",
    "watch:test": "TERM=dumb yarn test -o --watch",
    "prebuild": "yarn lint test security poststop",
    "build": "pm2 start process.json --env production",
    "postbuild": "yarn watch:app",
    "security": "nsp check",
    "watch:app": "tail -n +1 -f logs/app-err.log logs/app-out.log"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "transformIgnorePatterns": [
      "node_modules/(?!@material)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:learning-concepts/react.git"
  },
  "keywords": [
    "javascritp",
    "react",
    "router",
    "redux",
    "thunk",
    "saga"
  ],
  "author": "Berchris Requiao",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/learning-concepts/react/issues"
  },
  "homepage": "https://gitlab.com/learning-concepts/react#README",
