{
  "name": "redux-react",
  "version": "1.0.0",
  "description": "React and Redux example",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node src/server/server.prestart.js",
    "start": "npm-run-all --parallel lint:watch test:watch server:run",
    "server:run": "nodemon src/server/server.js",
    "lint": "esw src",
    "lint:watch": "yarn run lint --watch",
    "test": "jest",
    "test:watch": "TERM=dumb yarn test -o --watch",
    "security": "nsp check",
    "build": "NODE_ENV=production babel-node src/server/server.js"
  },
  "jest": {
    "rootDir": "src",
    "setupTestFrameworkScriptFile": "./setupTests.js",
    "automock": false
  },
  "nodemonConfig": {
    "ignore": [
      "*.test.js"
    ],
    "verbose": true,
    "execMap": {
      "js": "babel-node"
    },
    "watch": [
      "src/server/",
      "src/builder/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:learning-concepts/react.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Berchris Requiao",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/learning-concepts/react/issues"
  },
  "homepage": "https://gitlab.com/learning-concepts/react#README",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.3.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.2",
    "css-loader": "^0.28.10",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-watch": "^3.1.4",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "morgan": "^1.9.0",
    "nock": "^9.2.3",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
