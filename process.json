{
  "apps" : [
    {
      "name"        : "app",
      "script"      : "src/server/server.js",
      "interpreter" : "node_modules/.bin/babel-node",
      "exec_mode"   : "fork",
      "autorestart" : false,
      "watch"       : ["src/server", "src/builder"],
      "ignore_watch": ["*.test.js"],
      "env": {
        "NODE_ENV": "development"
      },
      "env_production" : {
        "NODE_ENV": "production"
      },
      "error_file": "logs/app-err.log",
      "out_file": "logs/app-out.log",
      "merge_logs": true
  }]
}
